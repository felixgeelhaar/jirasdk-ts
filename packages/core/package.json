{
  "name": "@felixgeelhaar/sdk-core",
  "version": "0.1.0",
  "description": "Shared core for Atlassian Cloud API SDKs - auth, transport, errors, and common types",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./auth": {
      "import": {
        "types": "./dist/auth/index.d.ts",
        "default": "./dist/auth/index.js"
      },
      "require": {
        "types": "./dist/auth/index.d.cts",
        "default": "./dist/auth/index.cjs"
      }
    },
    "./transport": {
      "import": {
        "types": "./dist/transport/index.d.ts",
        "default": "./dist/transport/index.js"
      },
      "require": {
        "types": "./dist/transport/index.d.cts",
        "default": "./dist/transport/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors/index.d.ts",
        "default": "./dist/errors/index.js"
      },
      "require": {
        "types": "./dist/errors/index.d.cts",
        "default": "./dist/errors/index.cjs"
      }
    },
    "./schemas": {
      "import": {
        "types": "./dist/schemas/index.d.ts",
        "default": "./dist/schemas/index.js"
      },
      "require": {
        "types": "./dist/schemas/index.d.cts",
        "default": "./dist/schemas/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "@fortify-ts/bulkhead": ">=0.1.0",
    "@fortify-ts/circuit-breaker": ">=0.1.0",
    "@fortify-ts/rate-limit": ">=0.1.0",
    "@fortify-ts/retry": ">=0.1.0",
    "@fortify-ts/timeout": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@fortify-ts/circuit-breaker": {
      "optional": true
    },
    "@fortify-ts/rate-limit": {
      "optional": true
    },
    "@fortify-ts/bulkhead": {
      "optional": true
    },
    "@fortify-ts/retry": {
      "optional": true
    },
    "@fortify-ts/timeout": {
      "optional": true
    }
  },
  "devDependencies": {
    "esbuild": "0.27.1",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "keywords": [
    "atlassian",
    "jira",
    "confluence",
    "sdk",
    "typescript",
    "api",
    "core"
  ],
  "license": "MIT",
  "author": "Felix Geelhaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixgeelhaar/jirasdk-ts.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false
}
