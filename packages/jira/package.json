{
  "name": "@felixgeelhaar/jira-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Atlassian Jira REST API with Zod validation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./schemas": {
      "import": {
        "types": "./dist/schemas/index.d.ts",
        "default": "./dist/schemas/index.js"
      },
      "require": {
        "types": "./dist/schemas/index.d.cts",
        "default": "./dist/schemas/index.cjs"
      }
    },
    "./services": {
      "import": {
        "types": "./dist/services/index.d.ts",
        "default": "./dist/services/index.js"
      },
      "require": {
        "types": "./dist/services/index.d.cts",
        "default": "./dist/services/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@felixgeelhaar/sdk-core": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "0.27.1",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  },
  "keywords": [
    "jira",
    "atlassian",
    "sdk",
    "typescript",
    "api",
    "rest",
    "client",
    "zod"
  ],
  "license": "MIT",
  "author": "Felix Geelhaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixgeelhaar/jirasdk-ts.git",
    "directory": "packages/jira"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false
}
